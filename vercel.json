{
    "version": 2,
    "builds": [
      {
        "src": "*.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "(.*)",
        "dest": "main.py"
      }
    ],
    "build": {
      "env": {
        "PIP_TARGET": ".pip_target"
      }
    },
    "functions": {
      "api/*.py": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }