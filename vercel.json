{
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python",
        "config": { "runtime": "python3.9" }
      },
      {
        "src": "build.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "public"
        }
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "main.py" }
    ],
    "env": {
      "PYTHONUNBUFFERED": "1"
    }
  }